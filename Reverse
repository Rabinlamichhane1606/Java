import java.util.Scanner;
public class Reverse
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter a number");
        int n=sc.nextInt();
        int a=n%10;
        int sum=0;
        sum=sum+a*100;
        n=n/10;
        a=n%10;
        sum=sum+a*10;
        n=n/10;
        sum=sum+n;
        System.out.println("Reverse is"+sum);
    }
}
